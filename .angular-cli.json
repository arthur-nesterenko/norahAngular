{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "norah"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "assets/css/toastr.css"
      ],
      "scripts": [
        "assets/libs/jquery.js",
        "assets/libs/jquery.mobilemenu.js",
        "assets/libs/superfish.js",
        "assets/libs/three.js",
        "assets/libs/Detector.js",
        "assets/libs/TrackballControls.js",
        "assets/libs/MTLLoader.js",
        "assets/libs/dat.gui.min.js",
        "assets/libs/OBJLoader2.js",
        "assets/libs/WWOBJLoader2.js",
        "assets/libs/FileSave.js",
        "assets/libs/UnityLoader.js",
        "assets/libs/UnityProgress.js",
        "assets/libs/toastr.js",
        "assets/libs/tm-scripts.js",
        "../node_modules/bootstrap-sass/assets/javascripts/bootstrap.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
